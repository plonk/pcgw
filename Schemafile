create_table "channels", force: true do |t|
  t.string  "gnu_id"
  t.integer "user_id"
end

add_index "channels", ["user_id"], name: "index_channels_on_user_id"

create_table "users", force: true do |t|
  t.string  "name"
  t.string  "image"
  t.integer "twitter_id"
  t.boolean "admin",      default: false
end
